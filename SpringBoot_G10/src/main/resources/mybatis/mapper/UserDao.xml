<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.himedia.g10.dao.IUserDao">

    <!-- id: 맵핑될 인터페이스 내에 있는 추상메서드 이름-->
    <select id="select" resultType="com.himedia.g10.dto.UserDto">
        select * from myuser
        /* select 실행의 결과가 여러개라면 resultType은 자동으로 List 형태가 된다 */
    </select>

</mapper>